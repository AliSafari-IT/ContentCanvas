{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5286"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "CC": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "http://localhost:3000", // Change this if you want a different startup page
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "iisExpress": {
        "applicationUrl": "http://localhost:56596", // Change this to the desired application URL
        "sslPort": 44300 // Set this to a valid SSL port if you're using HTTPS
      }
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:56596", // Ensure this matches the URL in the CC profile if needed
      "sslPort": 0 // Or set to the same SSL port as in the CC profile
    }
  }
}
